{
    "name": "cli-monorepo",
    "version": "1.0.5",
    "description": "YMA CLI 工具包",
    "main": "index.js",
    "scripts": {
        "init": "lerna clean -y && lerna bootstrap",
        "bootstrap": "lerna bootstrap",
        "clean": "lerna clean -y",
        "build": "lerna run build",
        "version": "lerna version --no-push --force-publish --conventional-prerelease --no-git-tag-version",
        "publish": "lerna publish from-package",
        "lint": "eslint . --ext .ts --config './config/.eslintrc.js' --ignore-path './config/.eslintignore'"
    },
    "devDependencies": {
        "@ecomfe/eslint-config": "7.4.0",
        "@types/node": "17.0.25",
        "typescript": "4.2.3"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:GitHubJian/yma-cli.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/GitHubJian/yma-cli/issues"
    },
    "homepage": "https://github.com/GitHubJian/yma-cli#readme",
    "workspaces": ["package/*"],
    "engines": {
        "node": ">=14.17.0"
    },
    "private": "true"
}
